                                            <Style TargetType="{x:Type TextBlock}">
                                                <Style.Triggers>
                                                    <Trigger Property="Text" Value="OFF">
                                                        <Setter Property="Foreground" Value="DarkRed"/>
                                                    </Trigger>
                                                    <Trigger Property="Text" Value="ON">
                                                        <Setter Property="Foreground" Value="DarkGreen"/>
                                                    </Trigger>
                                                </Style.Triggers>
                                            </Style>
                                        </TextBlock.Style>
                                    </TextBlock>

                                </Button>

                                <StackPanel Orientation="Horizontal" Margin="0,127,0,35">
                                    <Border x:Name="OVS_Display" Style="{StaticResource SHIP_DisplayNotifier}" HorizontalAlignment="Left"  Height="18" Tag="OFF">
                                        <TextBlock Text="OVS" HorizontalAlignment="Center" FontWeight="Bold" Foreground="Yellow" ToolTip="Oversize"/>
                                    </Border>

                                    <Border x:Name="AH_Display" Style="{StaticResource SHIP_DisplayNotifier}" HorizontalAlignment="Left"  Height="18" Tag="OFF">
                                        <TextBlock Text="AH" HorizontalAlignment="Center" FontWeight="Bold" Foreground="Yellow" ToolTip="Additional Handling"/>
                                    </Border>

                                    <Border x:Name="DAS_Display" Style="{StaticResource SHIP_DisplayNotifier}" HorizontalAlignment="Left"  Height="18" Tag="OFF">
                                        <TextBlock Text="DAS" HorizontalAlignment="Center" FontWeight="Bold" Foreground="Yellow" ToolTip="Delivery Area Surcharge"/>
                                    </Border>
                                </StackPanel>


                            </Grid>
                        </Border>
