                                                    <local:NumberTextBox x:Name="Twelve_Month_Comm_TxtBox" Margin="8,7,8,0" Grid.Column="3" Grid.Row="3" HorizontalAlignment="Center" Style="{DynamicResource ContactManager_Textbox}" Height="23" TextWrapping="Wrap" Text="$0.00" VerticalAlignment="Top" Width="59" TabIndex="12" Tag=""/>
                                                    <local:NumberTextBox x:Name="Twelve_Month_Other_TxtBox" Margin="8,7,8,0" Grid.Column="4" Grid.Row="3" HorizontalAlignment="Center" Style="{DynamicResource ContactManager_Textbox}" Height="23" TextWrapping="Wrap" Text="$0.00" VerticalAlignment="Top" Width="59" TabIndex="17" Tag=""/>



                                                    <Label Content="Total:" HorizontalAlignment="Right" VerticalAlignment="Top" Grid.Column="1" Grid.Row="4" Width="40"  Margin="0,5,0,0"/>
                                                    <Label Content="Monthly:" HorizontalAlignment="Right" VerticalAlignment="Bottom" Grid.Column="1" Grid.Row="4" Width="60"  Margin="0,0,0,4"/>
                                                    <TextBlock x:Name="_CRes" Grid.Column="2" Grid.Row="4" HorizontalAlignment="Left" VerticalAlignment="Center" Text="$0.00" Margin="18,33,0,6" Width="50" Height="16" FontFamily="Calibri"/>
