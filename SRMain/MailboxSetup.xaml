                                <ListBoxItem Content="Notices" Tag="InvoicePrinter"/>
                                <ListBoxItem Content="Fees and Deposits" Tag="ReportPrinter"/>
                                <ListBoxItem Content="Contract" Tag="DYMOLabelPrinter"/>
                            </ListBox>
                        </Border>

