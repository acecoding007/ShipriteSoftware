                                                <ListView.ItemContainerStyle>
                                                    <Style TargetType="ListViewItem">
                                                        <Setter Property="HorizontalContentAlignment" Value="Center"/>
                                                    </Style>
                                                </ListView.ItemContainerStyle>

                                                <ListView.View>
                                                    <GridView>
                                                        <GridViewColumn Header="Description" Width="155" DisplayMemberBinding="{Binding Description}"/>
                                                        <GridViewColumn Header="Start #" Width="55" DisplayMemberBinding="{Binding Starting_No}"/>
                                                        <GridViewColumn Header="End #" Width="55" DisplayMemberBinding="{Binding Ending_No}"/>
                                                    </GridView>
                                                </ListView.View>
                                            </local:CommonListView>
