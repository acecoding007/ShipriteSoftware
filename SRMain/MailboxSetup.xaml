                                                    <local:NumberTextBox x:Name="One_Month_Comm_TxtBox" Margin="7,18,8,13" Grid.Column="3" Grid.Row="0" HorizontalAlignment="Center" Style="{DynamicResource ContactManager_Textbox}" Height="23" TextWrapping="Wrap" Text="$0.00" VerticalAlignment="Center" Width="60" TabIndex="9" Tag=""/>
                                                    <local:NumberTextBox x:Name="One_Month_Other_TxtBox" Margin="8,18,7,13" Grid.Column="4" Grid.Row="0" HorizontalAlignment="Center" Style="{DynamicResource ContactManager_Textbox}" Height="23" TextWrapping="Wrap" Text="$0.00" VerticalAlignment="Center" Width="60" TabIndex="14" Tag=""/>


                                                    <TextBlock x:Name="_3Res" Grid.Column="2" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Center" Text="$0.00" Margin="18,33,0,6" Width="50" Height="16" FontFamily="Calibri"/>
