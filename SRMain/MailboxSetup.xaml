                                        <ComboBox x:Name="Cancellation_ComboBox" HorizontalAlignment="Left" Margin="12,242,0,0" VerticalAlignment="Top" Width="68" FontWeight="Bold" TabIndex="31">
                                            <ComboBoxItem Content="Before"/>
