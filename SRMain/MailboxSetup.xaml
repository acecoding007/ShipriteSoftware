                                                    <TextBlock x:Name="_12Com" Grid.Column="3" Grid.Row="3" HorizontalAlignment="Left" VerticalAlignment="Center" Text="$0.00" Margin="17,33,0,6" Width="50" Height="16" FontFamily="Calibri"/>
