                                                    <local:NumberTextBox x:Name="Three_Month_Comm_TxtBox" Margin="7,7,8,0" Grid.Column="3" Grid.Row="1"  Style="{DynamicResource ContactManager_Textbox}" Height="23" TextWrapping="Wrap" Text="$0.00"  VerticalAlignment="Top" Width="60" TabIndex="10" Tag=""/>
