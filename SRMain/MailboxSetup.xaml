                                                    <local:NumberTextBox x:Name="Custom_Comm_TxtBox" Margin="7,7,8,0" Grid.Column="3" Grid.Row="4" HorizontalAlignment="Center" Style="{DynamicResource ContactManager_Textbox}" Height="23" TextWrapping="Wrap" Text="$0.00" VerticalAlignment="Top" Width="60" TabIndex="13" Tag=""/>
                                                    <local:NumberTextBox x:Name="Custom_Other_TxtBox" Margin="8,7,7,0" Grid.Column="4" Grid.Row="4" HorizontalAlignment="Center" Style="{DynamicResource ContactManager_Textbox}" Height="23" TextWrapping="Wrap" Text="$0.00" VerticalAlignment="Top" Width="60" TabIndex="18" Tag=""/>


                                                    <Rectangle Grid.ColumnSpan="5" StrokeThickness="0.5" Fill="#FFF4F4F5" Height="1" Stroke="Black" VerticalAlignment="Top" Grid.Row="1" Margin="10,0,10,0"/>
                                                    <Rectangle Grid.ColumnSpan="5" StrokeThickness="0.5" Fill="#FFF4F4F5" Height="1" Stroke="Black" VerticalAlignment="Top" Grid.Row="2" Margin="10,0,10,0"/>
                                                    <Rectangle Grid.ColumnSpan="5" StrokeThickness="0.5" Fill="#FFF4F4F5" Height="1" Stroke="Black" VerticalAlignment="Top"  Grid.Row="3" Margin="10,0,10,0"/>
                                                    <Rectangle Grid.ColumnSpan="5" StrokeThickness="0.5" Fill="#FFF4F4F5"  Height="1" Stroke="Black" VerticalAlignment="Top"  Grid.Row="4" Margin="10,0,10,0"/>

                                                </Grid>
                                            </Border>

                                            <TextBox x:Name="Panel_Desc_TxtBox" HorizontalAlignment="Left" Style="{DynamicResource ContactManager_Textbox}" Height="23" Margin="17,43,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="157" FontSize="14" TabIndex="1" Tag="" Foreground="Black" FontWeight="Bold"/>
                                            <local:NumberTextBox x:Name="StartingNo_TxtBox" HorizontalAlignment="Left" Style="{DynamicResource ContactManager_Textbox}" Height="23" Margin="179,43,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="80" FontSize="14" TabIndex="2" Tag="" Foreground="Black" FontWeight="Bold"/>
                                            <TextBox x:Name="EndingNo_TxtBox" HorizontalAlignment="Left" Style="{DynamicResource ContactManager_Textbox}" Height="23" Margin="264,44,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="80" FontSize="14" TabIndex="3" Tag="" Foreground="Black" FontWeight="Bold"/>

