                                                    <Label Content="Monthly:" HorizontalAlignment="Right" VerticalAlignment="Bottom" Grid.Column="1" Grid.Row="1" Width="60"  Margin="0,0,0,4"/>
                                                    <Label Content="3 Months" HorizontalAlignment="Left" Grid.Column="0" Grid.Row="1"  VerticalAlignment="Center"  Width="70" FontWeight="Bold" Margin="35,14,0,15"/>
