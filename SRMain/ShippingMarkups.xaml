                                    <DataTemplate>
                                        <Image Margin="3" Source="{Binding CarrierImage}" Height="40" MaxWidth="75" RenderOptions.BitmapScalingMode="Fant" HorizontalAlignment="Center"/>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>
                        </Border>

