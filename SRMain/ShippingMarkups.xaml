                                                        <TextBlock Text="Location ID" HorizontalAlignment="Left" Margin="13,234,0,0" VerticalAlignment="Top" FontWeight="Normal"/>
                                                        <TextBlock Text="Agent ID" HorizontalAlignment="Left" Margin="26,255,0,0" VerticalAlignment="Top" FontWeight="Normal"/>
