                                                                    <Button x:Name="FedExREST_RegisterPIN_Btn" Content="Register" HorizontalAlignment="Left" Margin="322,242,0,0" VerticalAlignment="Top" Width="152" Height="23" FontWeight="Bold"/>
                                                                    <Label Content="Validation Via FedEx Tech Support" HorizontalAlignment="Left" Margin="20,271,0,0" VerticalAlignment="Top" Width="212" Height="26" FontWeight="Bold"/>
