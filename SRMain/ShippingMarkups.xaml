                                                                    <Button x:Name="FedExREST_RegisterTechSupport_Btn" Content="Register" HorizontalAlignment="Left" Margin="322,340,0,0" VerticalAlignment="Top" Width="152" Height="23" FontWeight="Bold"/>
                                                                    <TextBlock Text="Once FedEx Tech support provides authorization press the Register button." HorizontalAlignment="Left" Margin="27,333,0,-5" VerticalAlignment="Top" Width="290" Height="38" TextWrapping="Wrap" FontWeight="Bold" />
                                                                    <RadioButton x:Name="FedExREST_SMS_Radio" Content="Text Message" HorizontalAlignment="Left" Margin="20,210,0,0" VerticalAlignment="Top" FontWeight="Normal"/>
                                                                    <RadioButton x:Name="FedExREST_Phone_Radio" Content="Phone Call" HorizontalAlignment="Left" Margin="20,230,0,0" VerticalAlignment="Top" FontWeight="Normal"/>
                                                                    <RadioButton x:Name="FedExREST_Email_Radio" Content="Email" HorizontalAlignment="Left" Margin="20,250,0,0" VerticalAlignment="Top" FontWeight="Normal"/>

                                                                </Grid>
                                                            </Grid>
                                                        </TabItem>


                                                    </TabControl>
                                                </Border>
                                                <Border Background="#FFE5E5E5" BorderThickness="0" CornerRadius="10" HorizontalAlignment="Left" Height="382" VerticalAlignment="Top" Width="225">
                                                    <Grid>
                                                        <Label Content="FedEx FASC Discount Level" HorizontalAlignment="Left" VerticalAlignment="Top" Width="213"  Margin="10,112,0,0"/>
