                                                        <Label Content="FedEx Shipment Email Notifications" HorizontalAlignment="Left" VerticalAlignment="Top" Width="213" FontWeight="Bold" Margin="5,315,0,0"/>
                                                        <CheckBox x:Name="FedEx_DisableEmailNotification_ChkBx" Content="Check to Disable" HorizontalAlignment="Left" Margin="13,340,0,0" VerticalAlignment="Top" Width="203" FontWeight="Normal"/>
                                                        <Rectangle Fill="#FFF4F4F5" Height="1" Margin="0,315,0,0" Stroke="Black" VerticalAlignment="Top"/>
                                                        <Rectangle Fill="#FFF4F4F5" Height="1" Margin="0,276,0,0" Stroke="Black" VerticalAlignment="Top"/>
                                                        <Rectangle Fill="#FFF4F4F5" Height="1" Margin="0,360,0,0" Stroke="Black" VerticalAlignment="Top"/>
                                                        <Rectangle Fill="#FFF4F4F5" Height="1" Margin="0,207,0,0" Stroke="Black" VerticalAlignment="Top"/>
                                                        <Label Content="FedEx Account Number" HorizontalAlignment="Left" VerticalAlignment="Top" Width="213" FontWeight="Bold" Margin="1,1,0,0"/>
                                                        <TextBox x:Name="FedEx_AccountNumber_TextBox" HorizontalAlignment="Left" FontSize="14" Height="23" Margin="10,27,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="129" FontWeight="Normal"/>
