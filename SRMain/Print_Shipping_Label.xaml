                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="270"/>
                                                <ColumnDefinition Width="120"/>
                                                <ColumnDefinition Width="120"/>
                                            </Grid.ColumnDefinitions>

                                            <Grid.RowDefinitions>
                                                <RowDefinition/>
                                                <RowDefinition/>
                                                <RowDefinition/>
                                            </Grid.RowDefinitions>

                                            <TextBlock Text="{Binding contactAndAddress.contact.companyName}" Grid.Row="0" Grid.Column="0" FontWeight="Bold" />
                                            <TextBlock Text="{Binding contactAndAddress.contact.phoneNumber}" Grid.Row="0" Grid.Column="1" />

                                            <TextBlock  Grid.Row="1" Grid.Column="0" >
                                                <Run Text="{Binding contactAndAddress.address.streetLines[0], StringFormat='  {0}'}" />
                                                <Run Text="{Binding contactAndAddress.address.streetLines[1], StringFormat='  {0}'}" />
                                            </TextBlock>

                                            <TextBlock Grid.Row="2" Grid.Column="0">
                                                <Run Text="{Binding contactAndAddress.address.city, StringFormat='  {0},'}" />
                                                <Run Text="{Binding contactAndAddress.address.stateOrProvinceCode}" />
                                                <Run Text="{Binding contactAndAddress.address.postalCode}" />
                                            </TextBlock>


                                            <TextBlock Text="{Binding distance.value, StringFormat='Distance: {0} Miles'}" Grid.Row="0" Grid.Column="2" FontWeight="Bold" />
                                            <TextBlock Text="{Binding locationId, StringFormat='ID: {0}'}" Grid.Row="1" Grid.Column="2" TextAlignment="Left" FontSize="12"/>

                                        </Grid>
                                    </DataTemplate>
                                </ListBox.ItemTemplate>
                            </ListBox>
                        </Grid>
